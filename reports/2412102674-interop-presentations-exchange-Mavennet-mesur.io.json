      {
        "id": "d2fd68f6-a16e-4e0d-b733-f0c35a4a94a2",
        "name": "Get Organization DIDs (Issuer)",
        "request": {
          "url": {
            "path": [
              "identifiers",
              "{{ISSUER_ORGANIZATION_DID_WEB}}"
            ],
