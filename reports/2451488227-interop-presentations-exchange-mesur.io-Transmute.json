      {
        "id": "e2ee7d8d-5ce0-4490-ab10-fbd52255e525",
        "name": "Get Organization DIDs (Issuer)",
        "request": {
          "url": {
            "path": [
              "identifiers",
              "{{ISSUER_ORGANIZATION_DID_WEB}}"
            ],
