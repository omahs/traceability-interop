      {
        "id": "9446d0b0-3aaa-40eb-978a-8c776cf4cfe4",
        "name": "Get Organization DIDs (Issuer)",
        "request": {
          "url": {
            "path": [
              "identifiers",
              "{{ISSUER_ORGANIZATION_DID_WEB}}"
            ],
