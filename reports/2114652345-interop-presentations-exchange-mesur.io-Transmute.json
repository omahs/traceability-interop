        "id": "837fe978-189d-4e94-9ab2-60af5cd83c12",
        "name": "Get Issuer Organization DIDs",
        "request": {
          "url": {
            "path": [
              "identifiers",
              "{{ISSUER_ORGANIZATION_DID_WEB}}"
            ],
