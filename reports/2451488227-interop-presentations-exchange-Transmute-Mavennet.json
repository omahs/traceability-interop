      {
        "id": "d3b0b67e-4467-4bb6-9063-16ab0d4ee4c6",
        "name": "Get Organization DIDs (Issuer)",
        "request": {
          "url": {
            "path": [
              "identifiers",
              "{{ISSUER_ORGANIZATION_DID_WEB}}"
            ],
