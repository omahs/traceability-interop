      {
        "id": "3e6b2beb-67d4-4e9a-bf7f-2836a118c219",
        "name": "Get Organization DIDs (Issuer)",
        "request": {
          "url": {
            "path": [
              "identifiers",
              "{{ISSUER_ORGANIZATION_DID_WEB}}"
