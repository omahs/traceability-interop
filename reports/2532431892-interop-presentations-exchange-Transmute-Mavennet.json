      {
        "id": "2bb4cf7e-63d0-4eb0-928b-75cda6313b08",
        "name": "Get Organization DIDs (Issuer)",
        "request": {
          "url": {
            "path": [
              "identifiers",
              "{{ISSUER_ORGANIZATION_DID_WEB}}"
            ],
