      {
        "id": "2d5b20d3-0eeb-4a64-98b9-38e5b39c6c2d",
        "name": "Get Organization DIDs (Issuer)",
        "request": {
          "url": {
            "path": [
              "identifiers",
              "{{ISSUER_ORGANIZATION_DID_WEB}}"
            ],
