      {
        "id": "a61db42a-59eb-4d39-b9f6-4b036448a188",
        "name": "Sign Presentation",
        "request": {
          "url": {
            "path": [
              "presentations",
              "prove"
            ],
            "host": [
              "{{ISSUER_API_BASE_URL}}"
