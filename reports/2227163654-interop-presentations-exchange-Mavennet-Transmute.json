        "id": "8a3f01d7-2e17-4167-b981-84c2976a44d8",
        "name": "Get Issuer Organization DIDs",
        "request": {
          "url": {
            "path": [
              "identifiers",
              "{{ISSUER_ORGANIZATION_DID_WEB}}"
            ],
            "host": [
              "{{ISSUER_API_BASE_URL}}"
            ],
