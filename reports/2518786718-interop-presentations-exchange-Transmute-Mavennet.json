      {
        "id": "d53399e9-0c69-4edf-a73b-6bc027a5d8f6",
        "name": "Get Organization DIDs (Issuer)",
        "request": {
          "url": {
            "path": [
              "identifiers",
              "{{ISSUER_ORGANIZATION_DID_WEB}}"
            ],
