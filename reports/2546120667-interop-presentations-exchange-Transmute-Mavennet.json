      {
        "id": "1ab3d372-beb3-4d4c-b19d-367ac92de0b6",
        "name": "Get Organization DIDs (Issuer)",
        "request": {
          "url": {
            "path": [
              "identifiers",
              "{{ISSUER_ORGANIZATION_DID_WEB}}"
            ],
