      {
        "id": "56e14e79-ccf4-496e-aea9-ab77ad13bee2",
        "name": "Sign Presentation",
        "request": {
          "url": {
            "path": [
              "presentations",
              "prove"
            ],
            "host": [
              "{{ISSUER_API_BASE_URL}}"
