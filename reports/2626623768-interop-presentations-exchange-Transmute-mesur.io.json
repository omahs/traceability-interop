      {
        "id": "ac44f40b-16b1-47f6-a2b4-6f5d5de686cb",
        "name": "Get Organization DIDs (Issuer)",
        "request": {
          "url": {
            "path": [
              "identifiers",
              "{{ISSUER_ORGANIZATION_DID_WEB}}"
            ],
