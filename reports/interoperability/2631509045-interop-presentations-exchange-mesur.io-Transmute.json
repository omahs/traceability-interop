      {
        "id": "7bd42fc1-00e1-42bc-bcc9-547c2abbac2b",
        "name": "Get Organization DIDs (Issuer)",
        "request": {
          "url": {
            "path": [
              "identifiers",
              "{{ISSUER_ORGANIZATION_DID_WEB}}"
            ],
