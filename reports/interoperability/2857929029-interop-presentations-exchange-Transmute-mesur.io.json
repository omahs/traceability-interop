      {
        "id": "6f7fd5d3-046b-407d-bb73-7d6d20d7ce34",
        "name": "Get Organization DIDs (Issuer)",
        "request": {
          "url": {
            "path": [
              "identifiers",
              "{{ISSUER_ORGANIZATION_DID_WEB}}"
            ],
