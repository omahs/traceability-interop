      {
        "id": "5fefe053-a09f-45a0-8e58-ea4bbf8830d9",
        "name": "Get Organization DIDs (Issuer)",
        "request": {
          "url": {
            "path": [
              "identifiers",
              "{{ISSUER_ORGANIZATION_DID_WEB}}"
            ],
