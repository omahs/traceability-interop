      {
        "id": "63ca1f5b-1f1c-43a3-8c5f-4da9478af13e",
        "name": "Get Organization DIDs (Issuer)",
        "request": {
          "url": {
            "path": [
              "identifiers",
              "{{ISSUER_ORGANIZATION_DID_WEB}}"
            ],
