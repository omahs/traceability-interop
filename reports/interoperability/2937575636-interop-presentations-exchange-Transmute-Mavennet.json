      {
        "id": "d6e303ea-3c1b-4c9e-8a48-019ff70d6a02",
        "name": "Sign Presentation",
        "request": {
          "url": {
            "path": [
              "presentations",
              "prove"
            ],
            "host": [
              "{{ISSUER_API_BASE_URL}}"
