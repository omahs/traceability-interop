      {
        "id": "a1b2a8e0-9af5-4e5c-b153-b41d4c4c26f8",
        "name": "Get Organization DIDs (Issuer)",
        "request": {
          "url": {
            "path": [
              "identifiers",
              "{{ISSUER_ORGANIZATION_DID_WEB}}"
            ],
