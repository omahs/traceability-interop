      {
        "id": "aa0cf227-b0dc-4db1-ad3c-7ac01f161500",
        "name": "Get Organization DIDs (Issuer)",
        "request": {
          "url": {
            "path": [
              "identifiers",
              "{{ISSUER_ORGANIZATION_DID_WEB}}"
            ],
