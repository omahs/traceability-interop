      {
        "id": "df8b8541-d656-42fe-a8d1-fbe21ae356bf",
        "name": "Get Organization DIDs (Issuer)",
        "request": {
          "url": {
            "path": [
              "identifiers",
              "{{ISSUER_ORGANIZATION_DID_WEB}}"
            ],
