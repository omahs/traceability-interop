      {
        "id": "f2b405bf-11cc-42c4-9b3b-53fa98c989fa",
        "name": "Get Organization DIDs (Issuer)",
        "request": {
          "url": {
            "path": [
              "identifiers",
              "{{ISSUER_ORGANIZATION_DID_WEB}}"
            ],
