      {
        "id": "43a5dca6-d7f2-4d93-8e17-79ba6bb90399",
        "name": "Get Organization DIDs (Issuer)",
        "request": {
          "url": {
            "path": [
              "identifiers",
              "{{ISSUER_ORGANIZATION_DID_WEB}}"
            ],
