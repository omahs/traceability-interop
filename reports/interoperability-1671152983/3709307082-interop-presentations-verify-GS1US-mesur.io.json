        "id": "5abb9a6a-2f50-47ae-9c5d-72c6c2b1f0bb",
        "name": "Get Organization DIDs (Issuer)",
        "request": {
          "url": {
            "path": [
              "identifiers",
              "{{ISSUER_ORGANIZATION_DID_WEB}}"
            ],
