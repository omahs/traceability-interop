      {
        "id": "61486f0c-79d9-47d7-933b-9fd42d9ced7d",
        "name": "Get Organization DIDs (Issuer)",
        "request": {
          "url": {
            "path": [
              "identifiers",
              "{{ISSUER_ORGANIZATION_DID_WEB}}"
            ],
