      {
        "id": "d1b23504-9c1b-46d7-a4d3-486cf61ed212",
        "name": "Get Organization DIDs (Issuer)",
        "request": {
          "url": {
            "path": [
              "identifiers",
              "{{ISSUER_ORGANIZATION_DID_WEB}}"
            ],
