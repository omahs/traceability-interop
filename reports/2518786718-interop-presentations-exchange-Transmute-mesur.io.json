      {
        "id": "3e004a2d-1e05-46ff-a062-1c2473e5f84e",
        "name": "Get Organization DIDs (Issuer)",
        "request": {
          "url": {
            "path": [
              "identifiers",
              "{{ISSUER_ORGANIZATION_DID_WEB}}"
            ],
