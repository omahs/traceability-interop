      {
        "id": "9d1d4b87-a090-41bb-add9-7fddd5fb9d16",
        "name": "Get Organization DIDs (Issuer)",
        "request": {
          "url": {
            "path": [
              "identifiers",
              "{{ISSUER_ORGANIZATION_DID_WEB}}"
            ],
