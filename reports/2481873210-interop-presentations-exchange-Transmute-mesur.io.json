      {
        "id": "b5303889-4d7c-4bb2-ae5d-3792a57d3b9a",
        "name": "Get Organization DIDs (Issuer)",
        "request": {
          "url": {
            "path": [
              "identifiers",
              "{{ISSUER_ORGANIZATION_DID_WEB}}"
            ],
