      {
        "id": "ccb7a056-3bc3-4aa8-ac80-01f1cebc39fe",
        "name": "Sign Presentation",
        "request": {
          "url": {
            "path": [
              "presentations",
              "prove"
            ],
            "host": [
              "{{ISSUER_API_BASE_URL}}"
