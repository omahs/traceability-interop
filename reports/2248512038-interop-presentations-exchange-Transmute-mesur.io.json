        "id": "9bceae1d-fc3b-4ceb-99f2-b61b958286bf",
        "name": "Get Issuer Organization DIDs",
        "request": {
          "url": {
            "path": [
              "identifiers",
              "{{ISSUER_ORGANIZATION_DID_WEB}}"
            ],
