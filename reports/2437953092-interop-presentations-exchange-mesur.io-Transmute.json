      {
        "id": "9e3fec59-5f0a-4131-8b2a-78aa4d42ff50",
        "name": "Get Organization DIDs (Issuer)",
        "request": {
          "url": {
            "path": [
              "identifiers",
              "{{ISSUER_ORGANIZATION_DID_WEB}}"
            ],
