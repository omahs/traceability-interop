      {
        "id": "bac8af85-95bf-41ef-a7a7-3aa330ff09fd",
        "name": "Get Organization DIDs (Issuer)",
        "request": {
          "url": {
            "path": [
              "identifiers",
              "{{ISSUER_ORGANIZATION_DID_WEB}}"
            ],
