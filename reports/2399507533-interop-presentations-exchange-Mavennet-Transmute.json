      {
        "id": "a9ac73bd-5b4b-4f08-9e20-d4c08be32b64",
        "name": "Get Organization DIDs (Issuer)",
        "request": {
          "url": {
            "path": [
              "identifiers",
              "{{ISSUER_ORGANIZATION_DID_WEB}}"
            ],
