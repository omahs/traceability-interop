      {
        "id": "9e0ddaa6-774d-4ef6-97e4-33764af3c674",
        "name": "Get Organization DIDs (Issuer)",
        "request": {
          "url": {
            "path": [
              "identifiers",
              "{{ISSUER_ORGANIZATION_DID_WEB}}"
            ],
